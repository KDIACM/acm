<script type="text/javascript" src="js/jquery.validate.js"></script>
<script type="text/javascript">
$().ready(function() {
	//validate user info form
	$("#frm_user_add").validate({
		errorElement: "em",
		rules: {
			txt_name: {
				required : true
			},
			txt_real_name: {
				required : true
			},
			txt_password: {
				required : true
			},
			conf_password: {
				required : true,
				equalTo : "#txt_password"
			},
			txt_mail: {
				required : true,
				email : true
			}
		},
		messages: {
			txt_name: {
				required : "Enter Name"
			},
			txt_real_name: {
				required : "Enter Real Name"
			},
			txt_password: {
				required : "Enter Password"
			},
			conf_password: {
				required : "Enter Confirm Password",
				equalTo : "Confirm Pasword Miss Matched"
			},
			txt_mail: {
				required : "Enter Email",
				email : "Incorrect Email"
			}
		},
		success: function(label) {
				label.text("").addClass("success");
		}
	
	    } 
	)
})
</script>

<style type="text/css">
	em.error {
	  background:url("images/unchecked.gif") no-repeat 5px 0px;
	  padding-left: 26px;
	}
	em.success {
	  background:url("images/checked.gif") no-repeat 5px 0px;
	  padding-left: 26px; 
	}
	em.error { color: red; }
	em.success { color: blue; }
	#warning { display: none; }
</style>

